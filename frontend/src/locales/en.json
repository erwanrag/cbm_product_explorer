  "help": {
    "title": "Help & Documentation",
    "subtitle": "Complete guide to using CBM Product Explorer",
    "purpose": {
      "title": "üß≠ What is CBM Product Explorer?",
      "text": "CBM Product Explorer is a product analysis and matching tool based on various keys (manufacturer, internal and external references). It allows grouping, filtering and comparing products by quality, CRN group, and sales or stock history."
    },
    "features": {
      "title": "üìå Main Features",
      "search": "üîé Search by cod_pro, ref_crn, refint, ref_ext",
      "grouping": "üß© Smart grouping via grouping_crn",
      "dashboard": "üìä Full dashboard: KPIs, stock, history, turnover",
      "export": "üì• Excel export, combined filters",
      "performance": "‚ö° Fast resolution using Grouping_crn_table"
    },
    "filters": {
      "title": "üß™ Available Filters",
      "cod_pro": "cod_pro: Unique CBM product code",
      "ref_crn": "ref_crn: Manufacturer reference",
      "ref_ext": "ref_ext: External (or GRC) reference",
      "refint": "refint: Internal CBM reference",
      "qualite": "qualite: Product quality (OE, OEM, PMQ, PMV)",
      "grouping_crn": "grouping_crn: Enables logical grouping by product family"
    },
    "grouping": {
      "title": "üß© Understanding grouping_crn",
      "description": "The grouping_crn field is a logical key that links multiple CBM products (cod_pro) together, even if they don't directly share the same ref_crn, as long as they are connected via transitive links.",
      "example": "Example: product A is linked to ref_crn X, product B to X and Y, and product C to Y ‚Üí A, B and C are grouped under the same grouping_crn.",
      "calculation": "This grouping is automatically computed from Bridge_cod_pro_ref_crn and propagated into Grouping_crn_table (with quality, refint, and ref_ext included)."
    },
    "contact": "For technical issues, contact: e.ragueneau@cbmcompany.com",
    "documentation": "Full Documentation",
    "tutorials": "Tutorials",
    "faq": "FAQ",
    "support": "Support"
