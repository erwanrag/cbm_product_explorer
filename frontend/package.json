{
  "name": "cbm-grc-matcher-frontend",
  "version": "2.0.0",
  "description": "Frontend React pour CBM GRC Matcher - Application d'analyse et matching produits",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite --mode development --port 5181 --host 0.0.0.0",
    "build": "vite build --mode production",
    "build:staging": "vite build --mode staging",
    "preview": "vite preview --port 5181 --host 0.0.0.0",
    "analyze": "vite build --mode production && npx vite-bundle-analyzer dist",
    "lint": "eslint src --ext .js,.jsx --fix --max-warnings 0",
    "lint:check": "eslint src --ext .js,.jsx --max-warnings 0",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "clean": "rimraf dist node_modules/.vite .eslintcache",
    "clean:all": "rimraf dist node_modules package-lock.json .eslintcache && npm install",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && npm install",
    "test": "echo \"Tests à configurer\" && exit 0",
    "test:coverage": "echo \"Coverage à configurer\" && exit 0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@mui/x-data-grid": "^6.19.8",
    "@tanstack/react-query": "^5.28.6",
    "@tanstack/react-query-devtools": "^5.28.6",
    "axios": "^1.6.8",
    "dayjs": "^1.11.10",
    "framer-motion": "^11.0.25",
    "lodash.debounce": "^4.0.8",
    "plotly.js": "^2.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.22.3",
    "react-toastify": "^10.0.5",
    "recharts": "^3.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "vite": "^5.2.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "vite",
    "cbm",
    "grc",
    "matcher",
    "frontend",
    "material-ui"
  ],
  "author": "CBM Team",
  "license": "UNLICENSED"
}
